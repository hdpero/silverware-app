{
  "name": "silverware/app",
  "description": "SilverWare Application.",
  "homepage": "https://github.com/praxisnetau/silverware-app",
  "keywords": [
    "silverware",
    "application",
    "silverstripe"
  ],
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Colin Tucker",
      "role": "Developer",
      "email": "colin@praxis.net.au",
      "homepage": "http://www.praxis.net.au"
    }
  ],
  "require": {
    "php": ">=5.6.0",
    "gdmedia/ss-auto-git-ignore": "~1.0",
    "silverstripe/admin": "1.0.x-dev",
    "silverstripe/asset-admin": "1.0.x-dev",
    "silverstripe/assets": "1.0.x-dev",
    "silverstripe/campaign-admin": "1.0.x-dev",
    "silverstripe/cms": "4.0.x-dev",
    "silverstripe/config": "1.0.x-dev",
    "silverstripe/framework": "4.0.x-dev",
    "silverstripe/graphql": "0.2.x-dev",
    "silverstripe/reports": "4.0.x-dev",
    "silverstripe/siteconfig": "4.0.x-dev",
    "silverstripe/versioned": "1.0.x-dev",
    "silverware/colorpicker": "^0.1@dev",
    "silverware/font-icons": "^0.1@dev",
    "silverware/navigation": "^0.1@dev",
    "silverware/open-graph": "^0.1@dev",
    "silverware/silverware": "^0.1@dev",
    "silverware/theme": "^0.1@dev"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7"
  },
  "scripts": {
    "post-update-cmd": [
      "GDM\\SSAutoGitIgnore\\UpdateScript::Go"
    ],
    "post-create-project-cmd": [
      "SilverWare\\App\\Dev\\Installer::handleEvent"
    ]
  },
  "autoload": {
    "psr-4": {
      "SilverWare\\App\\": "app/src/"
    }
  },
  "config": {
    "process-timeout": 600
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
